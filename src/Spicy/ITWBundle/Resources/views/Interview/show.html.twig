{% extends "SpicySiteBundle::layout_admin.html.twig" %}

{% block body %}
    <h1>Interview</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('spicy_admin_home') }}"> Admin Home </a>        
        <ul class="record_actions">
    <li>
        <a href="{{ path('itw') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('itw_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
