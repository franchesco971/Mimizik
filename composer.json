{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": "<5.7",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.3.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.8.*",
        "symfony/swiftmailer-bundle": "2.*",
        "symfony/monolog-bundle": "2.2.*",
        "sensio/distribution-bundle": "3.*",
        "sensio/framework-extra-bundle": "3.0.*",
        "sensio/generator-bundle": "2.4.*",
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.4.*",

        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "doctrine/data-fixtures": "@dev",
        "stof/doctrine-extensions-bundle":"dev-master",
        "friendsofsymfony/user-bundle": "1.3.*",
        "friendsofsymfony/rest-bundle": "2.0.0",
        "coresphere/console-bundle": "0.1.*",
        "symfony/twig-bundle": "2.4.*",
        "hwi/oauth-bundle": "0.4.*",
        "facebook/php-sdk-v4": "^5.5",
        "symfony/var-dumper": "3.3.*",
        "swiftmailer/swiftmailer": "5.*",
        "jms/serializer-bundle": "1.5.*"
        
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "alpha",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
